{
  "routing":{
    "dashboard": "/dashboard",
    "accountDetail": "/account/{stakeAddress}"
  },
  "provider": {
    "adastry": {
      "url": "https://api.pasklab.com",
      "endpoints": {
        "login": "/auth/login",
        "createUser": "/user",
        "accountList": "/account/list",
        "addAccount": "/account",
        "updateAccount": "/account",
        "deleteAccount": "/account/{stakeAddress}",
        "getAccount": "/account/{stakeAddress}",
        "getHistory": "/account/{stakeAddress}/history",
        "getTransactions": "/account/{stakeAddress}/transactions",
        "exportRewards": "/account/{stakeAddress}/export-rewards/{year}",
        "exportTransactions": "/account/{stakeAddress}/export-transactions/{year}"
      }
    }
  },
  "messages": {
    "failedFetch": "Failed to retrieve data"
  },
  "theme": {
    "colors": [
      "#0dcaf0",
      "#0d6efd",
      "#6610f2",
      "#6f42c1",
      "#d63384",
      "#dc3545",
      "#fd7e14",
      "#ffc107",
      "#198754",
      "#20c997"
    ]
  }
}
